    <%= render 'shared/user_sidebar' %>
    <div class="col-md-9">
      <%= render 'home_pages/questions' %>
    </div>
  </div>
<% else %>
<div class="jumbotron container">
  <h1 class="display-4">Knowte</h1>
  <p class="lead">分からない事を分からないままにしない知識共有サイト</p>
  <hr class="mr-4">
